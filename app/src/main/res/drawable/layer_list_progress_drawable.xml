<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient android:startColor="#c6e0fa"
                android:centerColor="#c6e0fa" android:centerY="0.75"
                android:endColor="#c6e0fa" android:angle="270" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient android:startColor="#c6e0fa"
                    android:centerColor="#c6e0fa" android:centerY="0.75"
                    android:endColor="#c6e0fa" android:angle="270" />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient android:startColor="#49a6d6"
                    android:centerColor="#49a6d6" android:centerY="0.75"
                    android:endColor="#49a6d6" android:angle="270" />
            </shape>
        </clip>
    </item>

</layer-list>

